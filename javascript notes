//<---Compare scopes of the words var and let KeyWords ---->
function checkScope(){
  "use strict";
  let i = "function scope";
  if(true){
    let i = "block scope";
    console.log("Block scope i is: ", i);
  }
  console.log("Function scope i is: ", i);
  return i;
}
console.log(checkScope());

/*
Var can be delared and used globally while let is only able to
use locally or in the block of code it's in, so if we run the
function as it is well get block scope on the console. If we
change it to let then we'll get function scope for the console.
// NOTE: let made the function run perfectly and assigned to the right scope
 */

 //<------Declare a read-only variable with the const keyword ----->
