  "name": "Jesus",
  "leg": 10,
  "tails": 4,
  "friends":["Jack, Frank, Annie, Joseph, Kevin"]
};
myDog.name = "Franky Valley";
myDog.tails = 2;
myDog.friends = "No friends";

console.log(myDog["tails"]);
console.log(myDog["name"]);

