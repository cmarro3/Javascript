//<-----use getter and stters to control access to an object --->
class Book {
  constructor(author) {
    this._author = author;
  }
  //getter
  get writer (){
    return this._author;
  }
  //setter
  set writer(updateAuthor){
    this._author = updateAuthor;
  }
}


function makeClassic (){
class Thermostat {
  constructor(temp) {
    this._temp = 5/9 * (temp - 32); // NOTE: _ means a private var
  }
  get temperture (){
    return this._temp;
  }
  set temperture (updatedTemp){
    this._temp = updatedTemp;
  }
}
  return Thermostat;
}
const Thermostat = makeClassic();
const thermos = new Thermostat(76);
let temp = thermos.temperture;
temp = thermos.temperture;
console.log(temp);
