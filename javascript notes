///<---------- Chaninig if and else statments --------->
function test(num){
if(num < 5){
  return "Tiny";
}
else if (num < 10) {
  return "Small";
}
else if (num < 15){
  return "Medium";
}
else if (num < 20) {
  return "Large";
}
else if (num >= 20){
  return "Huge";
}
  return "Change me";
}
console.log(test(7));
console.log(test(20));

