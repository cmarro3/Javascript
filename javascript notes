const stats = {
  max: 56.78,
  standard: 4.34,
  median: 34.54,
  mode: 23.87,
  min: -0.75,
  ave: 35.85
};
const half = (function() {


  return function half({max, min}){
    return (max + min) / 2.0;
  };
}) ();
console.log(stats);
console.log(half(stats));

