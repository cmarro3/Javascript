//<---Writing higher order arrow functions ------>
const num = [5.6, -9.8, 7.8, 5.5,-7.9, 5, 1, 2];

const squareList = (arr) => {
  const squreInt = arr.filter(num => Number.isInteger(num) && num > 0).map(x => x * x);
  return squreInt;
};

const squreInt = squareList(num);
console.log(squreInt);
// NOTE: whenever one function take another function as an arugment that when we use the arrow
// NOTE: we use filter to file through the numbers
// NOTE: the map is uses to pass a function that isnt a full on function

//<-----write a higher order arrow fuction ---- >
const increment = (function () {
  return function increment (number, value = 1){
    return number + value;
  };
}) ();
console.log(increment(5,2));
console.log(increment(5));
// NOTE: defulat paremeter kicks in when a argutment is undefined
// NOTE: when we set value to 1 the function will pass on
// NOTE: I know what it does it's hard to explain is all

//<------ Use the Rest operator with fucntion parameters --- >
const sums = (function (){
  return function sums(...args){
    return args.reduce((a,b) => a + b, 0);
  };
})();
console.log(sums(1,2,3)); // NOTE: result 6
console.log(sums(1,2,3,4,5)); // NOTE: result 15
// NOTE: ... is rest opterator and you ca name it whatever you like
// NOTE: we named our args and got rid of the other delcalaration
// NOTE: we can also add more numbers add it'll work
// NOTE: this rest opreator helps with adding more numbers into the console

//<----Use the spread opreator to evaulte arrays in-pace ------>
const array1 = ["JAN", "FEB", "MAR", "APR", "MAY"]
let array2;
(function() {
  array2 = [...array1];
  array1[0] = "Potato"
}) ();
console.log(array2);
// NOTE: the console prints out jan instead of potato

