const AVG_TEMP = {
  today: 77.5,
  tomorrow: 79
};

function getTemp(AVG_TEMP){
  "use stict";

  const { tomorrow: tempOfTommorrow} = AVG_TEMP;
  // NOTE: this is saiying get the tomorrow and assigned it to the temp of tomarrow
  return tempOfTommorrow;
}
console.log(getTemp(AVG_TEMP)); // should be 79

//<-----desturcting assigment with nested objects ----->
const LOCAL_FORCAST = {
  today: {min: 72, max: 83},
  tomarrow: {min: 73.3, max: 84.6}
};

function getMax(LOCAL_FORCAST){
  "use strict";

  const { tomarrow: {max : maxOfTomarrow }}= LOCAL_FORCAST  ;
  return maxOfTomarrow;
}
console.log(getMax(LOCAL_FORCAST));
